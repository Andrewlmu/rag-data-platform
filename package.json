{
  "name": "pe-analysis-typescript",
  "version": "1.0.0",
  "description": "Private Equity Data Analysis Platform - TypeScript with full async",
  "main": "dist/server.js",
  "scripts": {
    "dev": "concurrently \"npm run chromadb\" \"npm run dev:backend\" \"cd frontend && npm run dev\"",
    "dev:backend": "nodemon --exec tsx src/backend/server.ts",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "tsc && cd frontend && npm run build",
    "start": "node dist/backend/server.js",
    "test": "jest",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix",
    "format": "prettier --write 'src/**/*.{ts,tsx,json}'",
    "format:check": "prettier --check 'src/**/*.{ts,tsx,json}'",
    "index-docs": "node scripts/index-documentation.js",
    "chromadb": "./scripts/start-chromadb.sh"
  },
  "keywords": [
    "pe",
    "data-analysis",
    "typescript",
    "async",
    "gpt-5"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@langchain/community": "^0.0.10",
    "@langchain/openai": "^0.0.12",
    "bull": "^4.11.5",
    "chromadb": "^1.7.3",
    "chromadb-default-embed": "^2.14.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "langchain": "^0.0.209",
    "mammoth": "^1.6.0",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.20.1",
    "pdf-parse": "^1.1.1",
    "rate-limit-redis": "^4.2.3",
    "redis": "^4.7.1",
    "reductoai": "^0.12.0",
    "socket.io": "^4.6.0",
    "xlsx": "^0.18.5",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.3",
    "@types/pdf-parse": "^1.1.4",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "concurrently": "^8.2.2",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.6.2",
    "tsx": "^4.6.2",
    "typescript": "^5.3.2"
  }
}
